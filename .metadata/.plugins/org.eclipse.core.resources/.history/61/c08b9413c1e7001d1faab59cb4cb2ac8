package dao;

import java.sql.Connection;

public class MemberDAO {
	private MemberDAO( ) {}
	
	private static MemberDAO instance = new MemberDAO();
	
	public static MemberDAO getInstance() {
		return instance;
	}
	
	private Connection con;

	public void setConnection(Connection con) {
		this.con = con;
	}
	
	
	
	

}



































